{
    "loader":
    {
        "random":
        {
            "endswith":"",
            "folder":".\\..\\..\\Raw\\BallTracking\\",
            "value_loader":
            {
                "delimited_filename":
                {
                    "delimiter":"_",
                    "token_load":
                    {
                        "note": 0,
                        "values": [1]
                    }
                }
            },
            "repeats":
            [{
                "match_type":"partial_match_filename",
                "value_match":"ProChunk",
                "repeat_times":5
            }]
        }
    },

    "transforms":
    [
        {
            "name":"one-hot",
            "input":"values",
            "output":"values",
            "dictionary":["g1","d1","o1","f1","f2","o2","d2","g2","-1"]
        },
        {
            "name":"resize",
            "input":"frame",
            "output":"frame",
            "size":[240,135],
            "resize_mode":"stretch"  
        },
        {
            "name":"normalize_channels",
            "input":"frame",
            "output":"frame",
            "max":1.0,
            "min":0.0
        }
    ],

    "writer":
    {
        "basic_writer":
        {
            "folder":".\\Result\\",
            "video":"balltracking.avi",
            "output":"balltracking.json"
        }
    }
}
