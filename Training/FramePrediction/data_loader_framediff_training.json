{
    "transforms":
    [
        {
            "name":"random_video_loader",
            "endswith":".avi",
            "folder":".\\..\\..\\Data\\Processed\\FramePrediction\\Training\\",
            "outputs":["frame0","frame1","frame2"]
        },
        {
            "name":"add_random_number",
            "output":"seed"
        },
        {
            "name":"randomize_frame",
            "p_seed":"seed",
            "input":"frame0",
            "output":"frame0",
            "chance_no_change":0.3,
            "zoom_range":[0.8, 1.2],
            "rotation_range":5,
            "width_shift_range":0.15,
            "height_shift_range":0.05,
            "shear_range":0.05,
            "fill_mode":"nearest",
            "gaussian_noise":0.00,
            "white_noise":0.00,
            "saturation_chance":0.5,
            "saturation_max_amount":0.5,
            "swap_channels":true,
            "invert_channels":false,
            "vertical_flip":false,
            "horizontal_flip":true
        },
        {
            "name":"randomize_frame",
            "p_seed":"seed",
            "input":"frame1",
            "output":"frame1",
            "chance_no_change":0.3,
            "zoom_range":[0.8, 1.2],
            "rotation_range":5,
            "width_shift_range":0.15,
            "height_shift_range":0.05,
            "shear_range":0.05,
            "fill_mode":"nearest",
            "gaussian_noise":0.00,
            "white_noise":0.00,
            "saturation_chance":0.5,
            "saturation_max_amount":0.5,
            "swap_channels":true,
            "invert_channels":false,
            "vertical_flip":false,
            "horizontal_flip":true
        },
        {
            "name":"randomize_frame",
            "p_seed":"seed",
            "input":"frame2",
            "output":"frame2",
            "chance_no_change":0.3,
            "zoom_range":[0.8, 1.2],
            "rotation_range":5,
            "width_shift_range":0.15,
            "height_shift_range":0.05,
            "shear_range":0.05,
            "fill_mode":"nearest",
            "gaussian_noise":0.00,
            "white_noise":0.00,
            "saturation_chance":0.5,
            "saturation_max_amount":0.5,
            "swap_channels":true,
            "invert_channels":false,
            "vertical_flip":false,
            "horizontal_flip":true
        },
        {
            "name":"normalize_channels",
            "input":"frame0",
            "output":"frame0",
            "max":1.0,
            "min":0.0
        },
        {
            "name":"normalize_channels",
            "input":"frame1",
            "output":"frame1",
            "max":1.0,
            "min":0.0
        },
        {
            "name":"normalize_channels",
            "input":"frame2",
            "output":"frame2",
            "max":1.0,
            "min":0.0
        },
        {
            "name":"frame_difference",
            "inputs":["frame1","frame2"],
            "output":"frame2",
            "abs":true
        },
        {
            "name":"threshold",
            "input":"frame2",
            "output":"frame2",
            "threshold":0.1,
            "max":1.0,
            "min":0.0
        },
        {
            "name":"channel_max",
            "input":"frame2",
            "output":"frame2"
        }
    ]
}
