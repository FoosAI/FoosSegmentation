{
    "transforms":
    [
        {
            "name":"random_video_loader",
            "endswith":".avi",
            "folder":".\\..\\..\\Data\\Processed\\BallTracking\\Training\\",
            "output":"frame",
            "value_loader":
            {
                "json":
                {
                    "values_to_load":["values"]
                }
            },
            "repeats":[]
        },
        {
            "name":"randomize_frame",
            "input":"frame",
            "output":"frame",
            "chance_no_change":0.3,
            "zoom_range":[0.8, 1.2],
            "rotation_range":5,
            "width_shift_range":0.15,
            "height_shift_range":0.05,
            "shear_range":0.05,
            "fill_mode":"nearest",
            "gaussian_noise":0.05,
            "white_noise":0.02,
            "saturation_chance":0.5,
            "saturation_max_amount":0.5,
            "swap_channels":true,
            "invert_channels":false,
            "vertical_flip":false,
            "horizontal_flip":true
        },
        {
            "name":"require",
            "input":"values",
            "input_index":8,
            "condition":"equals",
            "value":[0]
        },
        {
            "name":"normalize_channels",
            "input":"frame",
            "output":"frame",
            "max":1.0,
            "min":0.0
        }
    ]
}
