{
    "transforms":
    [
        {
            "name":"random_video_loader",
            "endswith":"",
            "folder":".\\Raw\\BallTracking\\",
            "output":"frame",
            "inclusions":["ProChunk"],
            "value_loader":
            {
                "delimited_filename":
                {
                    "filename":"filename",
                    "delimiter":"_",
                    "token_load":
                    {
                        "note": 0,
                        "values": [1]
                    }
                }
            },
            "repeats":
            [
                {
                    "match_type":"partial_match_filename",
                    "value_match":"ProChunk",
                    "repeat_times":2
                },
                {
                    "match_type":"partial_match_filename",
                    "value_match":"Nationals",
                    "repeat_times":2
                },
                {
                    "match_type":"partial_match_filename",
                    "value_match":"Worlds",
                    "repeat_times":2
                },
                {
                    "match_type":"partial_match_filename",
                    "value_match":"TKOSpreMoore",
                    "repeat_times":2
                },
                {
                    "match_type":"partial_match_filename",
                    "value_match":"2018TKO13",
                    "repeat_times":2
                },
                {
                    "match_type":"partial_match_filename",
                    "value_match":"_g",
                    "repeat_times":4.7
                },
                {
                    "match_type":"partial_match_filename",
                    "value_match":"_d",
                    "repeat_times":1.54
                },
                {
                    "match_type":"partial_match_filename",
                    "value_match":"_f",
                    "repeat_times":1.55
                },
                {
                    "match_type":"partial_match_filename",
                    "value_match":"TableView_-2",
                    "repeat_times":3
                }
            ]
        },
        {
            "name":"require",
            "input":"frame"
        },
        {
            "name":"replace",
            "input_output":"values",
            "replacements":
            [
                {
                    "from":["g1"],
                    "to":["d1"]
                },
                {
                    "from":["g2"],
                    "to":["d2"]
                }
            ]
        },
        {
            "name":"one-hot",
            "input":"values",
            "output":"values",
            "dictionary":["g1","d1","o1","f1","f2","o2","d2","g2","-2"]
        },
        {
            "name":"resize",
            "input":"frame",
            "output":"frame",
            "size":[135,240],
            "resize_mode":"stretch"  
        },
        {
            "name":"basic_writer",
            "frame_to_output":"frame",
            "values_to_output":["values","filename"],
            "folder":".\\Processed\\BallTracking\\Validation",
            "filename_prefix":"balltracking",
            "frames_per_chunk":10000
        }
    ]
}
