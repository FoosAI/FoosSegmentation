{
    "transforms":
    [
        {
            "name":"bing_image_loader",
            "queries":["inside","interior","landscape","activity","active","table","desk","office","billiards"],
            "folder":".\\Raw\\RawMatchesContinuous\\",
            "output":"frame",
            "min_width":500,
            "max_width":700,
            "min_height":250,
            "max_height":300,
            "image_type":"photo",
            "random":true,
            "license":"Public",
            "search_count":150,
            "search_pages":5
        },
        {
            "name":"resize",
            "input":"frame",
            "output":"frame",
            "size":[256,512],
            "resize_mode":"stretch"  
        },
        {
            "name":"show_frame",
            "input":"frame",
            "display":"Frame"
        },
        {
            "name":"basic_writer",
            "frame_to_output":"frame",
            "values_to_output":[],
            "folder":".\\Processed\\RandomImages\\",
            "filename_prefix":"images",
            "frames_per_chunk":10000
        }
    ]
}
