{
    "transforms":
    [
        {
            "name":"random_video_loader",
            "exclusions":[],
            "endswith":"images_0.mp4",
            "folder":".\\..\\..\\Data\\Raw\\RawMatchesContinuous\\",
            "output":"random_image",
            "random":true,
            "value_loader":
            {
            },
            "repeats":
            [
            ]
        },
        {
            "name":"random_video_loader",
            "endswith":".mp4",
            "folder":".\\..\\..\\Data\\Raw\\RawMatchesContinuous\\",
            "output":"frame",
            "exclusions":[],
            "random":true,
            "value_loader":
            {
                "json":
                {
                    "constant":true,
                    "values_to_load":["rod_lines", "score_lines", "table"]
                }
            },
            "repeats":
            [
                {
                    "match_type":"partial_match_filename",
                    "value_match":"images",
                    "repeat_times":10
                }
            ]
        },      
        {
            "name":"require",
            "input":"frame"
        },
        {
            "name":"require",
            "input":"random_image"
        },
        {
            "name":"resize_to_other",
            "input":"random_image",
            "output":"random_image",
            "size_reference":"frame",
            "resize_mode":"stretch"  
        },
        {
            "name":"merge_two_frames_by_polygon",
            "background_frame":"random_image",
            "frame_to_add":"frame",
            "output":"frame_modified_background",
            "region_to_add":"table"
        },
        {
            "name":"select_random",
            "inputs":["frame_modified_background","frame"],
            "weights":[1,3],
            "output":"frame"
        },
        {
            "name":"zeros_like",
            "reference":"frame",
            "output":"segmentation"
        },
        {
            "name":"draw_polygon",
            "input_frame":"segmentation",
            "input_polygon":"table",
            "fill":true,
            "width":1,
            "color":[255,0,0]
        },
        {
            "name":"resize",
            "input":"frame",
            "output":"frame",
            "size":[256,512],
            "resize_mode":"stretch"  
        },
        {
            "name":"resize",
            "input":"segmentation",
            "output":"segmentation",
            "size":[256,512],
            "resize_mode":"stretch"  
        },
        {
            "name":"add_random_number",
            "output":"seed"
        },
        {
            "name":"randomize_frame",
            "p_seed":"seed",
            "input":"frame",
            "output":"frame",
            "chance_no_change":0.3,
            "zoom_range":[0.8, 1.2],
            "rotation_range":5,
            "width_shift_range":0.15,
            "height_shift_range":0.05,
            "shear_range":0.05,
            "fill_mode":"nearest",
            "gaussian_noise":0.03,
            "white_noise":0.03,
            "saturation_chance":0.5,
            "saturation_max_amount":0.5,
            "swap_channels":true,
            "invert_channels":false,
            "vertical_flip":false,
            "horizontal_flip":true
        },
        {
            "name":"randomize_frame",
            "p_seed":"seed",
            "input":"segmentation",
            "output":"segmentation",
            "chance_no_change":0.3,
            "zoom_range":[0.8, 1.2],
            "rotation_range":5,
            "width_shift_range":0.15,
            "height_shift_range":0.05,
            "shear_range":0.05,
            "fill_mode":"nearest",
            "gaussian_noise":0.00,
            "white_noise":0.00,
            "saturation_chance":0.5,
            "saturation_max_amount":0.0,
            "swap_channels":false,
            "invert_channels":false,
            "vertical_flip":false,
            "horizontal_flip":true
        },
        {
            "name":"normalize_channels",
            "input":"frame",
            "output":"frame",
            "max":1.0,
            "min":0.0
        },
        {
            "name":"normalize_channels",
            "input":"segmentation",
            "output":"segmentation",
            "max":1.0,
            "min":0.0
        }
    ]
