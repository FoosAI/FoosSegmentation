{
    "transforms":
    [
        {
            "name":"single_video_loader",
            "webcam_number":0,
            "output":"frame"
        },
        {
            "name":"require",
            "input":"frame"
        },
        {
            "name":"resize",
            "input":"frame",
            "output":"frame_small",
            "size":[134,240],
            "resize_mode":"stretch"  
        },
        {
            "name":"normalize_channels",
            "input":"frame_small",
            "output":"frame_small",
            "max":1.0,
            "min":0.0
        },
        {
            "name":"run_model",
            "input":"frame_small",
            "model_file":"models_315.model",
            "output":"rod_onehot"
        },
        {
            "name":"onehot_to_name",
            "input":"rod_onehot",
            "output":"rod",
            "output_prob":"probability",
            "map":["Close Goalie","Close Defense","Close Offense","Close 5-Bar","Far 5-Bar","Far Offense","Far Defense","Far Goalie","Out of Play"]
        },
        {
            "name":"frame_write_text",
            "input_output":"frame",
            "line":0,
            "text_from_value":"rod"
        },
        {
            "name":"frame_write_text",
            "input_output":"frame",
            "line":1,
            "text_from_value":"probability"
        },
        {
            "name":"show_frame",
            "input":"frame",
            "display":"Rod Control"
        },
        {
            "name":"basic_writer",
            "frame_to_output":"frame",
            "values_to_output":[],
            "folder":".",
            "filename_prefix":"webcam",
            "frames_per_chunk":10000000000
        }
    ]
}
