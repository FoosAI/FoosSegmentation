{
    "transforms":
    [
        {
            "name":"single_video_loader",
            "video_file":"e:\\Foosball Videos\\TKO\\2018 TKO 11.mp4",
            "output":"frame"
        },
        {
            "name":"require",
            "input":"frame"
        },
        {
            "name":"resize",
            "input":"frame",
            "output":"frame_small",
            "size":[134,240],
            "resize_mode":"stretch"  
        },
        {
            "name":"normalize_channels",
            "input":"frame_small",
            "output":"frame_small_normalized",
            "max":1.0,
            "min":0.0
        },
        {
            "name":"run_model",
            "input":"frame_small_normalized",
            "model_file":"models_3.model",
            "output":"rod_onehot"
        },
        {
            "name":"onehot_to_name",
            "input":"rod_onehot",
            "output":"rod",
            "output_prob":"probability",
            "ignores":[8],
            "map":["Close Goalie","Close Defense","Close Offense","Close 5-Bar","Far 5-Bar","Far Offense","Far Defense","Far Goalie","Out of Play"]
        },
        {
            "name":"run_model",
            "input":"frame_small_normalized",
            "model_file":"scoremodel_100.model",
            "output":"score_onehot"
        },
        {
            "name":"onehot_to_name",
            "input":"score_onehot",
            "output":"score",
            "output_prob":"score_probability",
            "map":["0","1","2","3","4","5"]
        },
        {
            "name":"frame_write_text",
            "input_output":"frame",
            "line":0,
            "text_from_value":"rod"
        },
        {
            "name":"frame_write_text",
            "input_output":"frame",
            "line":1,
            "text_from_value":"probability"
        },
        {
            "name":"frame_write_text",
            "input_output":"frame",
            "line":2,
            "text_from_value":"score"
        },
        {
            "name":"frame_write_text",
            "input_output":"frame",
            "line":3,
            "text_from_value":"score_probability"
        },
        {
            "name":"frame_write_text",
            "input_output":"frame_small",
            "line":0,
            "text_from_value":"rod"
        },
        {
            "name":"frame_write_text",
            "input_output":"frame_small",
            "line":1,
            "text_from_value":"probability"
        },
        {
            "name":"show_frame",
            "input":"frame",
            "display":"Rod Control"
        },
        {
            "name":"basic_writer",
            "frame_to_output":"frame",
            "values_to_output":[],
            "folder":".",
            "filename_prefix":"video_large",
            "frames_per_chunk":10000000000
        },
        {
            "name":"basic_writer",
            "frame_to_output":"frame_small",
            "values_to_output":[],
            "folder":".",
            "filename_prefix":"video_small",
            "frames_per_chunk":10000000000
        }
    ]
}
