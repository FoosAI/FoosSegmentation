{
    "transforms":
    [
        {
            "name":"single_video_loader",
            "video_file":".\\..\\..\\Data\\Raw\\RawMatches\\2017 Tornado World Championships 1.mp4",
            "output":"frame"
        },
        {
            "name":"require",
            "input":"frame"
        },
        {
            "name":"resize",
            "input":"frame",
            "output":"frame",
            "size":[256,512],
            "resize_mode":"stretch"  
        },
        {
            "name":"normalize_channels",
            "input":"frame",
            "output":"frame",
            "max":1.0,
            "min":0.0
        },
        {
            "name":"run_model",
            "input":"frame",
            "model_file":"segmentation_models_276.h5",
            "output":"segmentation"
        },
        {
            "name":"channel_max",
            "input":"segmentation",
            "output":"segmentation",
            "max":1.0,
            "min":0.0
        },
        {
            "name":"contours_find",
            "input":"segmentation",
            "output_frame":"frame_with_contours",
            "output_contours":"contours_table",
            "threshold":0.5,
            "channel":2,
            "largest":1,
            "convert_to_convex_hull":false,
            "convert_to_bounding_rectangle":false,
            "convert_to_rotated_bounding_rectangle":false,
            "contour_min_area_pixels":10000
        },
        {
            "name":"contours_find",
            "input":"segmentation",
            "output_frame":"frame_with_contours",
            "output_contours":"contours_scores",
            "threshold":0.5,
            "channel":1,
            "largest":4,
            "convert_to_convex_hull":false,
            "convert_to_bounding_rectangle":false,
            "convert_to_rotated_bounding_rectangle":false,
            "contour_min_area_pixels":10
        },
        {
            "name":"contours_find",
            "input":"segmentation",
            "output_frame":"frame_with_contours",
            "output_contours":"contours_rods",
            "threshold":0.5,
            "largest":8,
            "channel":0,
            "convert_to_convex_hull":false,
            "convert_to_bounding_rectangle":false,
            "convert_to_rotated_bounding_rectangle":false,
            "contour_min_area_pixels":10
        },
        {
            "name":"contours_to_frames",
            "input_frame":"frame",
            "input_contours":"contours_table",
            "output_frames":["frame_table"],
            "output_num_frames":"frame_table_loaded",
            "output_frame_width":256,
            "output_frame_height":256
        },
        {
            "name":"draw_contours",
            "input_frame":"frame",
            "input_contours":"contours_table",
            "fill":false,
            "width":1,
            "color":[0,0,255]
        },
        {
            "name":"draw_contours",
            "input_frame":"frame",
            "input_contours":"contours_rods",
            "fill":false,
            "width":1,
            "color":[0,255,0]
        },
        {
            "name":"draw_contours",
            "input_frame":"frame",
            "input_contours":"contours_scores",
            "fill":false,
            "width":1,
            "color":[255,0,0]
        },
        {
            "name":"concat_frames",
            "inputs":["frame","segmentation","frame_table"],
            "output":"frame"
        },
        {
            "name":"show_frame",
            "input":"frame",
            "display":"Frame"
        },
        {
            "name":"show_frame",
            "input":"frame_table",
            "display":"Table"
        },
        {
            "name":"channel_max",
            "input":"frame_table",
            "output":"frame_table",
            "max":1.0,
            "min":0.0
        },
        {
            "name":"to_int_frame",
            "input":"frame_table",
            "output":"frame_table"
        },
        {
            "name":"channel_max",
            "input":"frame",
            "output":"frame",
            "max":1.0,
            "min":0.0
        },
        {
            "name":"to_int_frame",
            "input":"frame",
            "output":"frame"
        },
        {
            "name":"basic_writer",
            "frame_to_output":"frame",
            "values_to_output":[],
            "folder":".",
            "filename_prefix":"segmentation",
            "frames_per_chunk":1500
        },
        {
            "name":"basic_writer",
            "frame_to_output":"frame_table",
            "values_to_output":[],
            "folder":".",
            "filename_prefix":"extracted_table",
            "frames_per_chunk":1500
        }
    ]
}
